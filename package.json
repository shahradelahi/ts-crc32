{
  "name": "@se-oss/crc32",
  "version": "1.0.0",
  "description": "A high-performance, dependency-free library for calculating CRC32 and CRC32C checksums in TypeScript.",
  "keywords": [
    "crc",
    "crc32",
    "crc32c",
    "checksum",
    "hash",
    "stream"
  ],
  "homepage": "https://github.com/shahradelahi/ts-crc32",
  "repository": "github:shahradelahi/ts-crc32",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "!**/*.d.cts"
  ],
  "scripts": {
    "bench": "vitest bench --run",
    "build": "pnpm typecheck && tsup",
    "clean": "git clean -dfx node_modules dist .tsbuildinfo",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm build && pnpm lint && pnpm format:check && pnpm test",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "devDependencies": {
    "@shahrad/eslint-config": "^1.0.1",
    "@shahrad/prettier-config": "^1.2.2",
    "@shahrad/tsconfig": "^1.2.0",
    "@types/fast-crc32c": "^2.0.2",
    "@types/node": "^24.10.1",
    "crc-32": "^1.2.2",
    "eslint": "^9.39.1",
    "fast-crc32c": "^2.0.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "optionalDependencies": {
    "sse4_crc32": "^7.0.0"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
